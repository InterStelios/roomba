{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "it": true,
    "describe": true,
    "beforeEach": true
  },
  "rules": {
    "semi": 2,
    "no-unused-vars": 2,
    "no-undef": 2
  }
}
